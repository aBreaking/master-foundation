<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:component-scan base-package="com.abreaking"></context:component-scan>
    <context:annotation-config/>

    <bean id="po1" class="com.abreaking.master.spring.prototype.PrototypeObject" scope="prototype">
        <property name="name" value="zhangsan"></property>
    </bean>

    <bean id="po2" class="com.abreaking.master.spring.prototype.PrototypeObject" >
        <property name="name" value="lisi"></property>
    </bean>

</beans>